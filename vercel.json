{
  "buildCommand": null,
  "outputDirectory": "docs",
  "installCommand": null,
  "framework": null,
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- docs/",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

